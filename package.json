{
  "name": "angular-webpack",
  "version": "0.0.1",
  "description": "Angular starting point with webpack",
  "license": "MIT",
  "author": "Tyler Knipfer <knipferrc@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knipferrc/angular-parcel.git"
  },
  "bugs": {
    "url": "https://github.com/knipferrc/angular-parcel/issues"
  },
  "keywords": [
    "typescript",
    "angular",
    "webpack",
    "boilerplate"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development --config config/webpack.dev.js",
    "build-app": "webpack --mode production --config config/webpack.prod.js",
    "build": "npm run build-app",
    "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "format": "prettier --write '**/*.{tsx,ts,css,md,js}'"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "^7.2.1",
    "@ngtools/webpack": "^7.2.2",
    "@types/node": "^10.12.18",
    "clean-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "raw-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "typescript": "3.2.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@angular/animations": "^7.2.1",
    "@angular/common": "^7.2.1",
    "@angular/compiler": "^7.2.1",
    "@angular/core": "^7.2.1",
    "@angular/forms": "^7.2.1",
    "@angular/http": "^7.2.1",
    "@angular/platform-browser": "^7.2.1",
    "@angular/platform-browser-dynamic": "^7.2.1",
    "@angular/router": "^7.2.1",
    "core-js": "^2.6.2",
    "rxjs": "^6.3.3",
    "systemjs": "^3.0.0",
    "zone.js": "^0.8.28"
  }
}

{
  "name": "angular-webpack",
  "version": "0.0.1",
  "description": "Angular starting point with webpack",
  "license": "MIT",
  "author": "Tyler Knipfer <knipferrc@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knipferrc/angular-parcel.git"
  },
  "bugs": {
    "url": "https://github.com/knipferrc/angular-parcel/issues"
  },
  "keywords": [
    "typescript",
    "angular",
    "webpack",
    "boilerplate"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development --config config/webpack.dev.js",
    "build-app": "webpack --mode production --config config/webpack.prod.js",
    "build": "npm run build-app",
    "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "format": "prettier --write '**/*.{tsx,ts,css,md,js}'"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.2",
    "@angular/language-service": "^6.0.2",
    "@ngtools/webpack": "^6.0.1",
    "@types/node": "^10.1.0",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.12.1",
    "raw-loader": "^0.5.1",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "typescript": "2.7.2",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@angular/animations": "^6.0.2",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/router": "^6.0.2",
    "core-js": "^2.5.6",
    "rxjs": "^6.1.0",
    "systemjs": "^0.21.3",
    "zone.js": "^0.8.26"
  }
}
